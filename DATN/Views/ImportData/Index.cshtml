
@model DATN.Models.ClassTemp
@{
	Layout = null;
}

<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Đăng nhập</title>
	<link href="~/Assets/css/StyleLogin.css" rel="stylesheet" />
	<link href="~/Assets/Select2/select2.min.css" rel="stylesheet" />
	<script src="https://code.jquery.com/jquery-3.3.1.js" type="text/javascript"></script>
	<script src="~/Assets/Select2/select2.min.js"></script>
	<link rel="shortcut icon" href="~/Assets/images/seo.png" />
</head>
<body>

	<h1>@ViewBag.maso</h1>
	<h1>@ViewBag.thongbao</h1>
	@Html.Raw(TempData["file"]);
	<div class="login-wrap">
		<div class="login-html">
			<div class="login-form">

				<input id="tab-2" type="radio" name="tab" class="sign-up" checked><label for="tab-2" class="tab">Đăng nhập</label>
				<div class="login-form">
					@using (Html.BeginForm("uploadDiem", "ImportData", FormMethod.Post, new { enctype = "multipart/form-data" }))
					{
						@Html.AntiForgeryToken();
						<div class="sign-up-htm">
							<div class="group">
								@Html.Raw(TempData["Login_ERR"])
							</div>
							<div class="group">
								<label for="user" class="label">Lớp</label>
								@Html.TextBoxFor(model => model.lops)
								<label for="user" class="label">Mã SV</label>
								@Html.TextBoxFor(model => model.masv)
								<label for="user" class="label">File Điểm</label>
								<input type="file" class="file-upload-default file-click" name="excelfile" />

							</div>

							<div class="group">
								<input type="submit" class="button btn-google" value="Login Google+">
							</div>
							<div class="hr"></div>
						</div>
					}

				</div>
			</div>
		</div>
	</div>
</body>
</html>

